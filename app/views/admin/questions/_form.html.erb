<%= form_with(model: [:admin, @test, @question], local: true) do |form| %>
  <%= render 'shared/error', resource: @question %>

  <div>
    <%= form.label :body %>
    <%= form.text_field :body, class: 'form-control' %>
  </div>
  <div>
    <br>
    <%= form.submit class: "actions btn btn-primary" %>
  </div>
<% end %>
